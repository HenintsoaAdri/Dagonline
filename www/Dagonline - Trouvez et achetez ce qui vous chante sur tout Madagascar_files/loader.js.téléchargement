(function() {
  // Localize jQuery variable
  var jQuery;
  var jQuery_UI;

  // Load jQuery if not already available
  //if (window.jQuery === undefined || window.jQuery.fn.jquery !== '1.7.1') {
  if (window.jQuery === undefined) {
    var script_tag = document.createElement('script');
    script_tag.setAttribute("type", "text/javascript");
    script_tag.setAttribute("src", "https://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js");

    if (script_tag.readyState) {
      script_tag.onreadystatechange = function () { // For old versions of IE
        if (this.readyState == 'complete' || this.readyState == 'loaded') {
          scriptLoadHandler();
        }
      };
    } else {
      script_tag.onload = scriptLoadHandler;
    }

    // Try to find the head, otherwise default to the documentElement
   (document.getElementsByTagName("head")[0] || document.documentElement).appendChild(script_tag);

  } else {
    // The jQuery version on the window is the one we want to use
    jQuery = window.jQuery;

    // Check for additional Libraries for Widget Configurator
    
          
       main();
     
           
  }

  function scriptLoadHandler() {
    jQuery = window.jQuery.noConflict();

     // Check for additional Libraries for Widget Configurator
    
         
    main();
  }

  // Load additional Libraries for Widget Configurator
  function checkForAdditionalLibraries() {

    // Create additional scripts for widget configurator
    var script_tag_ui = document.createElement('script');
    script_tag_ui.setAttribute("type", "text/javascript");
    script_tag_ui.setAttribute("src", "https://code.jquery.com/ui/1.10.3/jquery-ui.js");
    (document.getElementsByTagName("head")[0] || document.documentElement).appendChild(script_tag_ui);

    var script_tag_jscolor = document.createElement('script');
    script_tag_jscolor.setAttribute("type", "text/javascript");
    script_tag_jscolor.setAttribute("src", "https://vivget.com/assets/js/jscolor/jscolor.js");
    (document.getElementsByTagName("head")[0] || document.documentElement).appendChild(script_tag_jscolor);
  }

  // Our code
  function main() {

    jQuery(document).ready(function($) {

      // Custom per widget-code
      var elements = 12;
var nextDiv = 1;
var isMouseOver = false;
var interval = null;
var functionNextDiv = null;


jQuery(document).ready(function() {

  functionNextDiv = function displayNextDiv() {

    if (isMouseOver == false) {
      jQuery('#zodiac_'+nextDiv+'_9562924fe5d9b82ae31281764aaca511').removeClass('active');

      currDiv = nextDiv;
      if (nextDiv < elements) {
        nextDiv++;
      } else {
        nextDiv = 1;
      }

      // Next line does actually nothing, but needed for animate in IE
      jQuery('#zodiac_'+currDiv+'_9562924fe5d9b82ae31281764aaca511').css('marginLeft','0');

      jQuery('#arrowLeft_9562924fe5d9b82ae31281764aaca511').unbind();
      jQuery('#arrowRight_9562924fe5d9b82ae31281764aaca511').unbind();

      jQuery('#zodiac_'+currDiv+'_9562924fe5d9b82ae31281764aaca511').animate({
        marginLeft: parseInt(jQuery('#zodiac_'+currDiv+'_9562924fe5d9b82ae31281764aaca511').css('marginLeft'),10) == 0 ?
        -jQuery('#zodiac_'+currDiv+'_9562924fe5d9b82ae31281764aaca511').outerWidth() :
        0
      },500, function() {
        jQuery('#zodiac_'+currDiv+'_9562924fe5d9b82ae31281764aaca511').hide();
        jQuery('#zodiac_'+nextDiv+'_9562924fe5d9b82ae31281764aaca511').addClass('active');
        width = jQuery('#zodiac_'+currDiv+'_9562924fe5d9b82ae31281764aaca511').width() + 25;
        jQuery('#zodiac_'+nextDiv+'_9562924fe5d9b82ae31281764aaca511').css('marginLeft','-'+width+'px');
        jQuery('#zodiac_'+nextDiv+'_9562924fe5d9b82ae31281764aaca511').show();
        jQuery('#zodiac_'+nextDiv+'_9562924fe5d9b82ae31281764aaca511').animate({
          marginLeft: parseInt(jQuery('#zodiac_'+currDiv+'_9562924fe5d9b82ae31281764aaca511').css('marginLeft'),10) == 0 ?
          +jQuery('#zodiac_'+nextDiv+'_9562924fe5d9b82ae31281764aaca511').outerWidth() :
          0
        }, 500, function() {
          jQuery('#arrowLeft_9562924fe5d9b82ae31281764aaca511').bind('click', lastZodiac);
          jQuery('#arrowRight_9562924fe5d9b82ae31281764aaca511').bind('click', nextZodiac);
        });
      });
    }

    clearInterval(interval);

    interval = setInterval(function() {
      functionNextDiv();
    }, 4500);
  }

  interval = setInterval(function() {
    functionNextDiv();
  }, 2000);

  jQuery('#showZodiacs_9562924fe5d9b82ae31281764aaca511').mouseover(function() {
    isMouseOver = true;
  });

  jQuery('#showZodiacs_9562924fe5d9b82ae31281764aaca511').mouseout(function() {
    isMouseOver = false;
  });

  jQuery('#arrowLeft_9562924fe5d9b82ae31281764aaca511').bind('click', lastZodiac);

  jQuery('#arrowRight_9562924fe5d9b82ae31281764aaca511').bind('click', nextZodiac);

  jQuery('.zodiacInfo_9562924fe5d9b82ae31281764aaca511').click(function() {
    if (jQuery('.showZodiacText_9562924fe5d9b82ae31281764aaca511').is(':visible')) {
      jQuery('.showZodiacText_9562924fe5d9b82ae31281764aaca511').fadeOut(1000).hide();
      interval = setInterval(function() {
        functionNextDiv();
      }, 2000);
    }
    else {
      clearInterval(interval);
      jQuery('.showZodiacText_9562924fe5d9b82ae31281764aaca511').fadeIn(1000);
    }
  });

});

function displayLastDiv() {
  if (isMouseOver == false) {
    jQuery('#zodiac_'+nextDiv+'_9562924fe5d9b82ae31281764aaca511').removeClass('active');

    currDiv = nextDiv;
    if (nextDiv > 1) {
      nextDiv--;
    } else {
      nextDiv = 12;
    }

    // Next line does actually nothing, but needed for animate in IE
    jQuery('#zodiac_'+currDiv+'_9562924fe5d9b82ae31281764aaca511').css('marginLeft','0');

    jQuery('#arrowLeft_9562924fe5d9b82ae31281764aaca511').unbind();
    jQuery('#arrowRight_9562924fe5d9b82ae31281764aaca511').unbind();

    jQuery('#zodiac_'+currDiv+'_9562924fe5d9b82ae31281764aaca511').animate({
      marginLeft: parseInt(jQuery('#zodiac_'+currDiv+'_9562924fe5d9b82ae31281764aaca511').css('marginLeft'),10) == 0 ?
      -jQuery('#zodiac_'+currDiv+'_9562924fe5d9b82ae31281764aaca511').outerWidth() :
      0
    },500, function() {
      jQuery('#zodiac_'+currDiv+'_9562924fe5d9b82ae31281764aaca511').hide();
      jQuery('#zodiac_'+nextDiv+'_9562924fe5d9b82ae31281764aaca511').addClass('active');
      width = jQuery('#zodiac_'+currDiv+'_9562924fe5d9b82ae31281764aaca511').width() + 25;
      jQuery('#zodiac_'+nextDiv+'_9562924fe5d9b82ae31281764aaca511').css('marginLeft','-'+width+'px');
      jQuery('#zodiac_'+nextDiv+'_9562924fe5d9b82ae31281764aaca511').show();
      jQuery('#zodiac_'+nextDiv+'_9562924fe5d9b82ae31281764aaca511').animate({
        marginLeft: parseInt(jQuery('#zodiac_'+currDiv+'_9562924fe5d9b82ae31281764aaca511').css('marginLeft'),10) == 0 ?
        +jQuery('#zodiac_'+nextDiv+'_9562924fe5d9b82ae31281764aaca511').outerWidth() :
        0
      }, 500, function() {
        jQuery('#arrowLeft_9562924fe5d9b82ae31281764aaca511').bind('click', lastZodiac);
        jQuery('#arrowRight_9562924fe5d9b82ae31281764aaca511').bind('click', nextZodiac);
      });
    });

  }

  clearInterval(interval);

  interval = setInterval(function() {
    displayLastDiv();
  }, 4500);
}

function lastZodiac() {
  clearInterval(interval);
  jQuery('#arrowLeft_9562924fe5d9b82ae31281764aaca511').unbind();
  jQuery('.showZodiacText_9562924fe5d9b82ae31281764aaca511').hide();
  currDiv = nextDiv;
  if (nextDiv > 1) {
    nextDiv -= 1;
  }
  else {
    nextDiv = 12;
  }
  jQuery('#zodiac_'+currDiv+'_9562924fe5d9b82ae31281764aaca511').hide(0, function() {
    jQuery('#zodiac_'+currDiv+'_9562924fe5d9b82ae31281764aaca511').removeClass('active');
    jQuery('#zodiac_'+currDiv+'_9562924fe5d9b82ae31281764aaca511').css('marginLeft','-300'+'px');
    jQuery('#zodiac_'+nextDiv+'_9562924fe5d9b82ae31281764aaca511').css('marginLeft','0');
    jQuery('#zodiac_'+nextDiv+'_9562924fe5d9b82ae31281764aaca511').show(0, function() {
      jQuery('#zodiac_'+nextDiv+'_9562924fe5d9b82ae31281764aaca511').addClass('active');
      jQuery('#arrowLeft_9562924fe5d9b82ae31281764aaca511').bind('click', lastZodiac);
      interval = setInterval(function() {
        displayLastDiv();
      }, 2000);
    });
  });
}

function nextZodiac() {
  clearInterval(interval);
  jQuery('#arrowRight_9562924fe5d9b82ae31281764aaca511').unbind();
  jQuery('.showZodiacText_9562924fe5d9b82ae31281764aaca511').hide();
  currDiv = nextDiv;
  if (nextDiv < elements) {
    nextDiv += 1;
  }
  else {
    nextDiv = 1;
  }
  jQuery('#zodiac_'+currDiv+'_9562924fe5d9b82ae31281764aaca511').hide(0, function() {
    jQuery('#zodiac_'+currDiv+'_9562924fe5d9b82ae31281764aaca511').removeClass('active');
    jQuery('#zodiac_'+currDiv+'_9562924fe5d9b82ae31281764aaca511').css('marginLeft','-300'+'px');
    jQuery('#zodiac_'+nextDiv+'_9562924fe5d9b82ae31281764aaca511').css('marginLeft','0');
    jQuery('#zodiac_'+nextDiv+'_9562924fe5d9b82ae31281764aaca511').show(0, function() {
      jQuery('#zodiac_'+nextDiv+'_9562924fe5d9b82ae31281764aaca511').addClass('active');
      jQuery('#arrowRight_9562924fe5d9b82ae31281764aaca511').bind('click', nextZodiac);
      interval = setInterval(function() {
        functionNextDiv();
      }, 2000);
    });
  });
}



    });
  }

})();

